					Terrain = ALL,
					NearTerrain = [],
					Distance = 1,
					Road = false,
					Clear = true,
					Force = true
				}
			],
			[
				1,
				{
					Script = "scripts/entity/world/attached_location/surface_iron_vein_location",
					Terrain = ALL,
					NearTerrain = [],
					Distance = 1,
					Road = false,
					Clear = true,
					Force = true
				}
			],
			[
