						Terrain = ALL,
						NearTerrain = [],
						Distance = 4,
						Road = true,
						Clear = true,
						Force = true
					}
				],
				[
					1,
					{
						Script = "scripts/entity/world/attached_location/ore_smelters_location",
						Terrain = ALL,
						NearTerrain = [],
						Distance = 0,
						Road = false,
						Clear = true,
						Force = true
					}
				],
				[
					1,
					{
						Script = "scripts/entity/world/attached_location/blast_furnace_location",
						Terrain = ALL,
						NearTerrain = [],
						Distance = 0,
						Road = false,
						Clear = true,
